{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "outDir": "./lib"
  },
  "include": ["./src"],
  "exclude":["node_modules", "lib"],
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": false
  }
}
